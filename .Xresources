!! *.font: xft:mono:medium:size=10

! special
*.foreground:   #e0f2ec
*.background:   #11292e
*.cursorColor:  #e0f2ec

! black
*.color0:       #0e1f22
*.color8:       #1c4b4e

! red
*.color1:       #a85659
*.color9:       #eb8995

! green
*.color2:       #668e8c
*.color10:      #82aea9

! yellow
*.color3:       #c3b6a4
*.color11:      #e1d9ce

! blue
*.color4:       #326d78
*.color12:      #a7d2cd

! magenta (or orange)
*.color5:       #433c32
*.color13:      #aa9576

! cyan
*.color6:       #386e74
*.color14:      #90c0bd

! white
*.color7:       #e0f2ec
*.color15: 		#f0f8f3

*.font:			FuraCode Nerd Font Mono:style=Regular:size=9

i3wm.background:				#515356
i3wm.foreground: 				#a7aeba

Xft.dpi:                        96
Xft.antialias:                  1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                  hintslight
Xft.lcdfilter:                  lcdlight
Xft.rgba:                       rgb

!urxvt.font:						xft:DejaVuSansMono Nerd Font
urxvt.font:						xft:FuraCode Nerd Font Mono:style=Regular:size=9
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       true
URxvt*cursorBlink:                true
URxvt*saveLines:                  2000
URxvt.perl-ext:
URxvt.perl-ext-common:
URxvt.letterSpace: -1

URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B